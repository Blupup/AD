using System; 
 
public class Specialty 
{ 
 private string name; 
 private string abbreviation; 
 private DateTime date; 
 
 public Specialty(string name, string sokra, DateTime? date = null) 
 { 
 name = name; 
 sokra = sokra; 
 date = date ?? DateTime.Today; 
 } 
 
 public string Name() 
 { 
 return name; 
 } 
 
 public string Sokr() 
 { 
 return abbreviation; 
 } 
 
 public DateTime Date() 
 { 
 return date; 
 } 
}
